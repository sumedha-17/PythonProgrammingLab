L1=[]
rangeofL1=int(input("enter the range of L1" ))
for i in range(rangeofL1):
    temp=int(input())
    L1.append(temp)
print(L1)

L2=[]
rangeofL2=int(input("enter the range of L2"))
for i in range(rangeofL2):
    temp=int(input())
    L2.append(temp)
print(L2)

